{
    "type": "project",
    "name": "zoe/as",
    "description": "Zukunftsorientierte Energie",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-zip": "*",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/annotations": "^2",
        "doctrine/doctrine-bundle": "^2.10.2",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.9",
        "endroid/qr-code-bundle": "^4.3",
        "google/apiclient": "^2.1",
        "google/cloud-speech": "^1.14",
        "knplabs/knp-snappy-bundle": "^1.9",
        "knplabs/knp-time-bundle": "^2",
        "league/commonmark": "*",
        "minishlink/web-push": "^9.0",
        "nelmio/cors-bundle": "^2.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpmailer/phpmailer": "^6.6",
        "phpstan/phpdoc-parser": "^1.4",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.4",
        "survos/barcode-bundle": "^1.4",
        "symfony/asset": "6.4.*",
        "symfony/config": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/doctrine-messenger": "6.4.*",
        "symfony/dom-crawler": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/event-dispatcher": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/filesystem": "6.4.*",
        "symfony/flex": "^2",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/google-mailer": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/intl": "6.4.*",
        "symfony/mailer": "6.4.*",
        "symfony/messenger": "6.4.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/notifier": "6.4.*",
        "symfony/password-hasher": "6.4.*",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/proxy-manager-bridge": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/slack-notifier": "6.4.*",
        "symfony/string": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/ux-chartjs": "^2.10",
        "symfony/ux-cropperjs": "^2.6",
        "symfony/validator": "6.4.*",
        "symfony/yaml": "6.4.*",
        "tattali/calendar-bundle": "^1.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.3",
        "twig/markdown-extra": "^3.3",
        "twig/string-extra": "^3.5",
        "twig/twig": "^2.12|^3.0",
        "web-token/jwt-framework": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true,
            "endroid/installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": ["@auto-scripts"],
        "post-update-cmd": ["@auto-scripts"],
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^7.1",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.18",
        "symfony/browser-kit": "6.4.*",
        "symfony/debug-bundle": "6.4.*",
        "symfony/phpunit-bridge": "6.4.*",
        "symfony/stopwatch": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*",
        "symfony/webpack-encore-bundle": "^1.16",
        "zenstruck/foundry": "^1.23"
    }
}
