<style>
    ul.breadcrumb li {
        float: right;
        padding: 0.5px;
        {#background-color: {{ sfColorLigth }};#}
        /*-moz-border-radius: 0 50px 50px 0;*/
        /*-webkit-border-radius:  0 50px 50px 0;*/
        /*border-radius:  0 50px 50px 0;*/
        position: relative;
        margin-left: -10px;
        -moz-transition: all 0.2s;
        -o-transition: all 0.2s;
        -webkit-transition: all 0.2s;
        transition: all 0.2s;
    }
    ul.breadcrumb li:last-child a::after, ul.breadcrumb li:last-child .a::after {
        content: "";
        display: block;
        position: absolute;
        top: 0;
        left: 100%;
        border-style: solid;
        border-color: transparent {{ sfColor }};
        border-width: 17px 0 15px 15px;
        width: 0;
        height: 0;
    }
    ul.breadcrumb li a, ul.breadcrumb li .a {
        overflow: hidden;
        /*-moz-border-radius:  0 50px 50px 0;*/
        /*-webkit-border-radius:  0 50px 50px 0;*/
        /*border-radius:  0 50px 50px 0;*/
        -moz-transition: all 0.2s;
        -o-transition: all 0.2s;
        -webkit-transition: all 0.2s;
        transition: all 0.2s;
        text-decoration: none;
        height: 30px;
        color: {{ sfColor }};
        background-color: {{ sfBG }};
        text-align: center;
        min-width: 50px;
        display: block;
        line-height: 30px;

        padding-left: 8px;
        padding-right: 50px;
        width: 50px;
    }
    ul.breadcrumb li a .material-icons, ul.breadcrumb li .a .material-icons {
        display: inline-block;
    }
    ul.breadcrumb li .a .text, ul.breadcrumb li .a .text {
        display: none;
        opacity: 0;
    }
    ul.breadcrumb li a:hover, ul.breadcrumb li.active a {
        width: fit-content !important;
        background-color: {{ sfColorFormular }} !important;
    }
    ul.breadcrumb li.active a {
        background-color: #fff !important;
        color: {{ sfColorDark }};
    }
    ul.breadcrumb li a:hover .text, ul.breadcrumb li.active a .text, a.a span {
        display: inline-block !important;
        opacity: 1 !important;
    }
    /*ul.breadcrumb li:last-child a {*/
    /*    padding: 0;*/
    /*}*/
    /*ul.breadcrumb li:last-child:hover {*/
    /*    padding: 3px;*/
    /*    margin-top: 0;*/
    /*}*/
    /*ul.breadcrumb li:last-child:hover a {*/
    /*    width: 60px;*/
    /*    height: 50px;*/
    /*    line-height: 60px;*/
    /*}*/
    @media (min-width: 768px) {
        ul.breadcrumb li a:hover, ul.breadcrumb li.active a {
            width: max-content !important;
            background-color: #e6f3f3 !important;
        }
    }
    @media (max-width: 767px) {
        .hidden-xs {
            display: none !important;
        }
        ul.breadcrumb li a:hover, ul.breadcrumb li.active a {
            width: 0 !important;;
            background-color: #e6f3f3 !important;
        }
    }
</style>
