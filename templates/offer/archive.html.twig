{% extends 'base.html.twig' %}

{% block title %}{{ 'title.offer.archive'|trans }}{% endblock %}
{% block stylesheets %}
    {{ parent() }}
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('app/js/ui.js') }}" type="text/javascript"></script>
{% endblock %}
{% block main %}
    <div class="row">
        <div class="col-md-12" style="border-radius: 10px;">
            <div class="table-responsive" style="background-color: transparent;">
                <div id="page-wrapper" data-controller="email">
                    {% include 'offer/_offer_block_cover.html.twig' with {'block':'archive','offers': inquiries, 'storno':false} %}
                    {% include 'offer/modals/_email_modal.html.twig' %}
                </div>
            </div>
        </div>
    </div>
{% endblock %}
