
<table class="mt5">
    <tr>
        <td>
            <h3 class="inline-block">Wetter</h3>
        </td>
        <td colspan="3">
            <p class="inline-block" style="width: 20px;padding-left: 18px;"><input style="width: 100%" class="check_i" type="radio" name="pv_first[mess][3]" {% if yes and ocp.mess[3] is defined and ocp.mess[3] == 1 %}checked{% endif %} value="1"></p>
            <p class="inline-block" style="width: 90px;padding-left: 12px">Sonnig</p>
            <p class="inline-block" style="width: 20px;padding-left: 18px;"><input style="width: 100%" class="check_i" type="radio" name="pv_first[mess][3]" {% if yes and ocp.mess[3] is defined and ocp.mess[3] == 2 %}checked{% endif %} value="2"></p>
            <p class="inline-block" style="width: 150px;padding-left: 12px">Wechselhaft</p>
            <p class="inline-block" style="width: 20px;"><input style="width: 100%" class="check_i" type="radio" name="pv_first[mess][3]" {% if yes and ocp.mess[3] is defined and ocp.mess[3] == 3 %}checked{% endif %} value="3"></p>
            <p class="inline-block" style="width: 60%;padding-left: 12px"><input class="check_i" type="text" name="pv_first[mess][4]" value="{% if yes and ocp.mess[4] is defined %}{{ ocp.mess[4] }}{% endif %}"></p>
        </td>
    </tr>
    <tr class="btTd">
        <td>
            <h3 class="inline-block">Umgebungsbedingung</h3>
        </td>
